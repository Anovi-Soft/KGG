<UserControl x:Class="KGG.KggCanvas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KGG"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="900">
    <Grid>
        <Rectangle Fill="#FFEAD5" HorizontalAlignment="Left" Height="600" Stroke="Black" VerticalAlignment="Top" Width="900"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="900" Source="Images/mask.png"/>
        <Image x:Name="maskArrow" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="900"/>

    </Grid>
</UserControl>
